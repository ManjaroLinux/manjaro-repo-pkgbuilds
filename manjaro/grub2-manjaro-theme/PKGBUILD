# Maintainer: ying

pkgname=grub2-manjaro-theme
pkgver=1.1
pkgrel=1
pkgdesc="Manjaro green boot theme for grub2"
arch=('any')
url="http://www.manjarolinux.org"
license=('GPL2')
source=('06_manjarotheme')

package() {
	mkdir -p ${pkgdir}/etc/grub.d
	install -Dm755 ${srcdir}/06_manjarotheme ${pkgdir}/etc/grub.d/06_manjarotheme
	chmod +x ${pkgdir}/etc/grub.d/06_manjarotheme
}


md5sums=('f003cba20459629899cb04f5522b473b')
